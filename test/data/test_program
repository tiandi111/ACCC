Class Base {
    a : Int;
    b : String;
    c : Bool;
    d : IO;

    do(arg1 : Int, arg2 : Int) : Int {
        {
            temp <- arg3 + arg2;
            d.out_int(temp / 2);
        }
    };
};

Class Derived1 inherits Base {
    do(arg1 : Int, arg2 : Int) : Int {
        {
            temp <- arg1 - arg2;
            d.out_int(temp / 2);
        }
    };
};